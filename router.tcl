source "routerlib.tcl"proc sys_receive {intf sock} {::receive $intf [read $sock]}proc interface_init {intf_list} {foreach intf $intf_list {    set ::handle($intf) [set sock [udp_open $cfg::port($intf)]]    fconfigure $sock  -translation binary -buffering none	fconfigure $sock -remote [list $cfg::remote_host($intf) $cfg::remote_port($intf)]    fileevent $sock readable "sys_receive $intf $sock"}}set server "router_server"variable chan1 [open router.log w]package require comm::comm::comm configure -port $cfg::port($server)interface_init "F1/0 F0/0"vwait forever